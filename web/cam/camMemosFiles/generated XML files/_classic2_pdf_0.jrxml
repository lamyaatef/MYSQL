	<queryString>
		<![CDATA[SELECT   gen_dcm.dcm_code as "DCM Code", gen_dcm.dcm_name as "DCM Name", shop.dcm_name as "Shop Name",

      SUM

            (DECODE (commission_detail.commission_type_category_id,

                     1, commission_item.commission_item_amount,

                     0

                    )

            ) total_lines,

         SUM

            (DECODE (commission_detail.commission_type_category_id,

                     1, commission_item_amount * commission_factor_value,

                     0

                    )

            ) total_amount,

         NVL

            (SUM (DECODE (commission_type_category_name,

                          'Authentication Penalty', ABS

                                    (  commission_item.commission_item_amount

                                     * commission_factor.commission_factor_value

                                    ),

                          0

                         )

                 ),

             0

            ) unreal_deductions,

         SUM (commission_item_amount * commission_factor_value) "Net Amount"

    FROM

    commission_detail,

         payment_detail,

         payment_commission,

         commission_type_category,

         commission_item,

         commission_factor,

         gen_dcm,

         cam_payment ,

         gen_Dcm shop,

         CCM_ADDANDWIN_SHOP



   WHERE

    cam_payment.payment_id = payment_detail.payment_detail_id

    AND CAM_PAYMENT.SCM_ID = GEN_DCM.DCM_ID

    AND CAM_PAYMENT.MEMO_ID = 149

    and cam_payment.flag='INCLUDED'

     AND payment_detail.payment_detail_id =

                                          payment_commission.payment_detail_id

     AND commission_detail.commission_detail_id =

                                       payment_commission.commission_detail_id

     AND commission_detail.commission_type_category_id =

                          commission_type_category.commission_type_category_id

     AND commission_detail.commission_detail_id =

                                          commission_item.commission_detail_id

     AND commission_item.commission_factor_id =

                                        commission_factor.commission_factor_id

    and commission_factor_value<>0

     AND commission_item.dcm_id = gen_dcm.dcm_id

     and

       CCM_ADDANDWIN_SHOP.ADDANDWIN_ID   (+) = gen_dcm.dcm_id

       and

        CCM_ADDANDWIN_SHOP.SHOP_ID = shop.dcm_id (+)

GROUP BY gen_Dcm.dcm_code, gen_dcm.dcm_name,shop.dcm_name

ORDER BY gen_Dcm.dcm_name]]>
	</queryString>
