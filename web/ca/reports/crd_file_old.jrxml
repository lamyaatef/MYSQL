	<parameter name="credit_advice_file_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
CRD_CREDIT_DETAILS.credit_serial,
CRD_CREDIT_DETAILS.date_of_payment,
CAM_MEMO.credit_advice_file_id,
CAM_MEMO.memo_name,
ACC_WITH_HOLD_TAX.with_hold_tax,
CRD_STATUS_CREDIT.status_name,
( SUM (CAM_PAYMENT.SCM_COMMISSION_VALUE)
- SUM (CAM_PAYMENT.DEDUCTIONS_VALUE))
AS AMOUNT,
NVL

(vw_dcm_grouping.dcm_code, GEN_DCM.dcm_code) AS parent_code,
vw_dcm_grouping.FRANCHISE_GROUP_NAME
,CRD_ADVICE_FILE.PRINTING
FROM CRD_CREDIT_DETAILS,
CAM_MEMO,
CAM_PAYMENT,
GEN_DCM,
ACC_WITH_HOLD_TAX,
CRD_STATUS_CREDIT,
vw_dcm_grouping
,CRD_ADVICE_FILE
WHERE CRD_CREDIT_DETAILS.credit_advice_id IN (SELECT CRD_CREDIT_DETAILS.credit_advice_id
FROM CRD_CREDIT_DETAILS,CAM_PAYMENT,CAM_MEMO
WHERE CAM_MEMO.credit_advice_file_id =$P{credit_advice_file_id}
AND CAM_PAYMENT.memo_id = CAM_MEMO.memo_id
AND CAM_PAYMENT.credit_advice_id =
CRD_CREDIT_DETAILS.credit_advice_id)
AND GEN_DCM.dcm_code = franchise_code(+)
AND GEN_DCM.DCM_ID = CAM_PAYMENT.SCM_ID
AND CAM_PAYMENT.credit_advice_id = CRD_CREDIT_DETAILS.credit_advice_id
AND CAM_PAYMENT.memo_id = CAM_MEMO.memo_id
AND ACC_WITH_HOLD_TAX.DCM_CODE = GEN_DCM.DCM_CODE
AND CRD_CREDIT_DETAILS.STATUS_ID = CRD_STATUS_CREDIT.STATUS_ID
AND CAM_MEMO.credit_advice_file_id =CRD_ADVICE_FILE.FILE_ID
GROUP

BY CRD_CREDIT_DETAILS.credit_serial,
CRD_CREDIT_DETAILS.date_of_payment,
CAM_MEMO.credit_advice_file_id,
CAM_MEMO.memo_name,
ACC_WITH_HOLD_TAX.with_hold_tax,
CRD_STATUS_CREDIT.status_name,
NVL(vw_dcm_grouping.dcm_code, GEN_DCM.dcm_code),
vw_dcm_grouping.FRANCHISE_GROUP_NAME
,CRD_ADVICE_FILE.PRINTING
]]>
