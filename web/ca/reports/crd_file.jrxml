	<parameter name="credit_advice_file_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[/* Formatted on 2/25/2013 1:31:45 PM (QP5 v5.185.11230.41888) */
SELECT credit_serial,
       CRD_CREDIT_DETAILS.date_of_payment,
       CAM_MEMO.credit_advice_file_id,
       CAM_MEMO.memo_title,
       GEN_DCM.dcm_code,
       DECODE (ACC_WITH_HOLD_TAX.with_hold_tax, 0.022, 0, with_hold_tax)
          with_hold_tax,
       CRD_STATUS_CREDIT.status_name,
       SUM (
            DECODE (
               with_hold_tax,
               0.022,   (  CAM_PAYMENT.SCM_COMMISSION_VALUE
                         - CAM_PAYMENT.DEDUCTIONS_VALUE)
                      * (1 + with_hold_tax)  ,
               (  CAM_PAYMENT.SCM_COMMISSION_VALUE
                  - CAM_PAYMENT.DEDUCTIONS_VALUE)
          / 0.95))
          AMOUNT,
       NVL( vw_dcm_grouping.dcm_code, GEN_DCM.dcm_code) AS parent_code,
NVL(vw_dcm_grouping.franchise_group_name, GEN_DCM.dcm_name) franchise_group_name , CRD_ADVICE_FILE.printing
FROM
CRD_CREDIT_DETAILS,
CAM_MEMO
,
CAM_PAYMENT
,
GEN_DCM
,
ACC_WITH_HOLD_TAX
,
CRD_STATUS_CREDIT
,
vw_dcm_grouping,
CRD_ADVICE_FILE
WHERE
CRD_CREDIT_DETAILS.credit_advice_id IN (
SELECT
CRD_CREDIT_DETAILS.credit_advice_id
FROM
CRD_CREDIT_DETAILS, CAM_PAYMENT, CAM_MEMO
WHERE
CAM_MEMO
.credit_advice_file_id =$P{credit_advice_file_id}
AND
CAM_PAYMENT.scm_commission_value <> 0
AND
CAM_PAYMENT.memo_id = CAM_MEMO.memo_id
AND
CAM_PAYMENT.credit_advice_id =
CRD_CREDIT_DETAILS
.credit_advice_id)
AND
GEN_DCM.dcm_code = franchise_code(+)
AND
GEN_DCM.dcm_id = CAM_PAYMENT.scm_id
AND
CAM_PAYMENT.credit_advice_id = CRD_CREDIT_DETAILS.credit_advice_id
AND
CAM_PAYMENT.memo_id = CAM_MEMO.memo_id
AND
ACC_WITH_HOLD_TAX.dcm_code = GEN_DCM.dcm_code
AND
CRD_CREDIT_DETAILS.status_id = CRD_STATUS_CREDIT.status_id
AND
CAM_MEMO.credit_advice_file_id = CRD_ADVICE_FILE.file_id
GROUP BY
CRD_CREDIT_DETAILS.credit_serial,
CRD_CREDIT_DETAILS
.printing,
CRD_CREDIT_DETAILS
.date_of_payment,
CAM_MEMO
.credit_advice_file_id,
CAM_MEMO
.memo_title,
GEN_DCM.dcm_code, DECODE (ACC_WITH_HOLD_TAX.with_hold_tax,0.022,0,with_hold_tax ),
CRD_STATUS_CREDIT
.status_name,
NVL(
vw_dcm_grouping.dcm_code, GEN_DCM.dcm_code),
NVL(
vw_dcm_grouping.franchise_group_name, GEN_DCM.dcm_name),
CRD_ADVICE_FILE
.printing
HAVING
(SUM (CAM_PAYMENT.scm_commission_value)-SUM (CAM_PAYMENT.deductions_value))/0.95 >0

ORDER BY NVL (vw_dcm_grouping.dcm_code, GEN_DCM.dcm_code)]]>
	</queryString>
